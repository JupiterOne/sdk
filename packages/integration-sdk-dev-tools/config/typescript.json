{
  "compilerOptions": {
    "incremental": true,
    "target": "es2021",
    "lib": ["es2021"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "pretty": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "allowJs": false
  }
}
