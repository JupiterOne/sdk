{{#with template}}
import { createIntegrationEntity } from '@jupiterone/integration-sdk-core';
import { Entities } from './constants';

{{#each steps}}
    {{#with entity}}
export function create{{pascalCase name}}Entity(data: any) {
  return createIntegrationEntity({
    entityData: {
      source: data,
      assign: {
        _key: data.{{_keyPath}},
        _type: Entities.{{constantCase name}}._type,
        _class: Entities.{{constantCase name}}._class,
        {{#if staticFields}}
          {{#each staticFields}}
        {{@key}}: {{{escape this}}},
          {{/each}}
        {{/if}}
        {{#if fieldMappings}}
          {{#each fieldMappings}}
        {{@key}}: data.{{this}},
          {{/each}}
        {{/if}}
      },
    },
  });
}
  {{/with}}

{{/each}}
{{/with}}