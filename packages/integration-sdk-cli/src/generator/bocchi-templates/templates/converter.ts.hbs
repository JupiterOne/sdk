{{#each converters}}
export function create{{pascalCase this.entityName}}Entity(data: any) {
    return createIntegrationEntity({
        entityData: {
            source: data,
            assign: {
                _key: data.{{this.keyField}},
                _type: Entities.{{constantCase this.entityName}}._type,
                _class: Entities.{{constantCase this.entityName}}._class,
                {{#if this.staticFields}}
                    {{#each this.staticFields}}
                        {{@key}}: {{this}},
                    {{/each}}
                {{/if}}
                {{#if this.staticFields}}
                    {{#each this.fieldthiss}}
                        {{@key}}: data.{{this}},
                    {{/each}}
                {{/if}}
            },
        },
    });
}
{{/each}}
